{
    "name": "joshhanley/livewire-autocomplete",
    "description": "An autocomplete dropdown for Livewire",
    "license": "MIT",
    "authors": [
        {
            "name": "Josh Hanley",
            "email": "josh@joshhanley.com.au"
        }
    ],
    "require": {
        "php": "^8.0",
        "laravel/framework": "^9.0|^10.0|dev-master",
        "livewire/livewire": "dev-feature/dusk-testable-in-packages as 3.0.3|^3.0"
    },
    "require-dev": {
        "spatie/laravel-ray": "^1.19",
        "phpunit/phpunit": "^9",
        "orchestra/testbench-dusk": "^7.0|^8.0",
        "orchestra/testbench": "^7.0|^8.0",
        "psy/psysh": "^0.11.18"
    },
    "autoload": {
        "psr-4": {
            "LivewireAutocomplete\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "LivewireAutocomplete\\Tests\\": "tests/",
            "Tests\\": "vendor/livewire/livewire/tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "LivewireAutocomplete\\ServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "repositories": {
        "livewire/livewire": {
            "type": "vcs",
            "url": "git@github.com:joshhanley/livewire.git"
        }
    }
}
